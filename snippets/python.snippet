snippet #!
	#! /usr/bin/env python
	# -*- coding: utf-8 -*-
snippet imp
	import ${0:module}
snippet from
	from ${1:package} import ${0:module}

snippet if
	if ${1:condition}:
		${0}
snippet ifelse
	if ${1:condition}:
		${0}
	else:
		${2}
snippet ifelif
	if ${1:condition}:
		${0}
	elif ${2:condition}:
		${3}
snippet while
	while ${1:condition}:
		${0}
